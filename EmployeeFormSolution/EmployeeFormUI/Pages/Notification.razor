@page "/notifications"
@using EmployeeFormUI.Services;
@using Azure.Messaging.ServiceBus;
@inherits NotificationBase

<center>
    <h3>Queue Messages</h3>
    <ul>
        @foreach (var message in messages)
        {
            <li>@message</li>
        }
    </ul>
    
        <button  @onclick="RetrieveMessages" class="btn btn-primary" type="submit">Retrieve Queue Messages</button>
    
</center>
